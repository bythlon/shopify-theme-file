{{ 'custom-detail-carousel.css' | asset_url | stylesheet_tag }}

 <!-- what makes section start from here -->
 <section class="whatmakes">
    <div class="page-width">
        <div class="whatmakesWrper">
            <div class="whatmakesWrperTop">
                <div class="text-info-wrapper">
                    <h1 class="whatmakesheadeing">
                        {{ section.settings.heading }}
                    </h1>
                </div>
                <div class="media-wrapper">
                  <!-- Slider main container -->
                  <div class="swiper deatailImagCarousel">
                    <!-- Additional required wrapper -->
                    <div class="swiper-wrapper">
                        <!-- Slides -->
                      {% for block in section.blocks %}
                        {% case block.type %}
                          {% when 'slide' %}
                            <div class="swiper-slide">
                              {%- if block.settings.image != blank -%}

                                {%- assign widths = '165, 360, 535, 750, 1070, 1500' -%}
                                {%- capture sizes -%}
                                  (min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 1.5 }}px,
                                  (min-width: 1070px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)
                                {%- endcapture -%}
                                {{
                                  block.settings.image
                                  | image_url: width: 1070
                                  | image_tag: loading: 'lazy', sizes: sizes, widths: widths
                                }}
                              {%- else -%}
                                {{ 'detailed-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                              {%- endif -%}
                            </div>
                        {% endcase %}
                      {% endfor %}
                    </div>
                    <!-- If we need pagination -->
                    {% comment %} <div class="swiper-pagination"></div> {% endcomment %}

                    <!-- If we need navigation buttons -->
                    <div class="swiper-button-next">
