{% schema %}
    {
      "name": "Detail Carousel",
      "tag": "section",
      "settings": [
        {
        "type": "textarea",
        "label": "Heading",
        "id": "heading",
        "default": "Heading"
        }
      ],
      "blocks": [
        {
        "type": "slide",
        "name": "Slide",
        "settings": [
          {
          "type": "image_picker",
          "label": "Image",
          "id": "image"
          },
          {
          "type": "text",
          "label": "Heading",
          "id": "heading",
          "default": "1"
          },
          {
            "type": "textarea",
            "label": "Content",
            "id": "content",
            "default": "Lorem ipsum dolor sit amet consectetur adipisicing elit. Ratione culpa ex repudiandae eos architecto voluptatum illum quaerat! Perferendis, minus quas."
          }
        ]
        }
      ],
      "presets": [
        {
          "name": "Detail Carousel",
          "blocks": [
            {
              "type": "slide"
            },
            {
              "type": "slide"
            },
            {
              "type": "slide"
            },
            {
              "type": "slide"
            },
            {
              "type": "slide"
            }
          ]
        }
      ]
    }
{% endschema %}